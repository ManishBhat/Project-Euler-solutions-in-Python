      "source": [
        "from sympy import isprime\n",
        "\n",
        "def f(i):\n",
        "  return (i+1)**3 - i**3\n",
        "\n",
        "def f2(N):\n",
        "  i = 1\n",
        "  p = f(i)\n",
        "  counter = 0\n",
        "  while p < N:\n",
        "    if isprime(p):\n",
        "      counter += 1\n",
        "    i += 1\n",
        "    p = f(i)\n",
        "  return counter"
      ],
      "execution_count": 3,
      "outputs": []
    },
